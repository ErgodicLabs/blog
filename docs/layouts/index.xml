      <pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate>
      <guid>https://ergodiclabs.github.io/blog/layouts/partials/footer/</guid>
      <description>&lt;footer class=&#34;footer&#34;&gt;&#xA;  &lt;div class=&#34;footer__inner&#34;&gt;&#xA;    {{ if $.Site.Copyright }}&#xA;      &lt;div class=&#34;copyright copyright--user&#34;&gt;&#xA;        &lt;span&gt;{{ $.Site.Copyright | safeHTML }}&lt;/span&gt;&#xA;    {{ else }}&#xA;      &lt;div class=&#34;copyright&#34;&gt;&#xA;        &lt;span&gt;Â© {{ now.Year }} Powered by &lt;a href=&#34;https://gohugo.io&#34;&gt;Hugo&lt;/a&gt;&lt;/span&gt;&#xA;    {{ end }}&#xA;      &lt;/div&gt;&#xA;  &lt;/div&gt;&#xA;&lt;/footer&gt;&#xA;&#xA;{{ $menu := resources.Get &#34;js/menu.js&#34; | js.Build }}&#xA;{{ $cp := resources.Get &#34;js/code.js&#34; | js.Build }}&#xA;&#xA;{{ $bundle := slice $menu $cp | resources.Concat &#34;bundle.js&#34; | resources.Minify }}&#xA;&#xA;&lt;script type=&#34;text/javascript&#34; src=&#34;{{ $bundle.RelPermalink }}&#34;&gt;&lt;/script&gt;&#xA;&#xA;&lt;!-- Extended footer section--&gt;&#xA;{{ partial &#34;extended_footer.html&#34; . }}</description>
    </item>
    <item>
