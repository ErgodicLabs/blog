      <description>&lt;header class=&#34;header&#34;&gt;&#xA;  &lt;div class=&#34;header__inner&#34;&gt;&#xA;    &lt;div class=&#34;header__logo&#34;&gt;&#xA;      {{ partial &#34;logo.html&#34; . }}&#xA;    &lt;/div&gt;&#xA;    {{ if len $.Site.Menus }}&#xA;      {{ partial &#34;mobile-menu.html&#34; . }}&#xA;    {{ end }}&#xA;    {{ if and $.Site.Params.showLanguageSelector (len $.Site.Home.AllTranslations) }}&#xA;      {{ partial &#34;language-menu.html&#34; . }}&#xA;    {{ end }}&#xA;  &lt;/div&gt;&#xA;  {{ if len $.Site.Menus }}&#xA;    {{ partial &#34;menu.html&#34; . }}&#xA;  {{ end }}&#xA;&lt;/header&gt;</description>
